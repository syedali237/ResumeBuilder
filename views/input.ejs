<%- include('partials/header') %>

<div class="container">
    <form>
        <label for="name">Name:</label>
        <input type="text" id="name" placeholder="Your Name">

        <label for="phone">Phone Number:</label>
        <input type="tel" id="phone" placeholder="Your Phone Number">

        <label for="email">Email:</label>
        <input type="email" id="email" placeholder="Your Email">

        <label for="linkedin">LinkedIn Link:</label>
        <input type="text" id="linkedin" placeholder="Your LinkedIn Link">

        <label for="about">About:</label>
        <textarea id="about" placeholder="About yourself"></textarea>

        <label for="education-college">Education (College):</label>
        <input type="text" id="education-college" placeholder="College Name">

        <label for="education-12th">Education (12th):</label>
        <input type="text" id="education-12th" placeholder="12th School Name">

        <label for="education-10th">Education (10th):</label>
        <input type="text" id="education-10th" placeholder="10th School Name">

        <label for="skills">Skills:</label>
        <input type="text" id="skills" placeholder="Your Skills">

        <label for="project1">Project 1:</label>
        <input type="text" id="project1" placeholder="Project 1">

        <label for="project2">Project 2:</label>
        <input type="text" id="project2" placeholder="Project 2">

        <label for="project3">Project 3:</label>
        <input type="text" id="project3" placeholder="Project 3">

        <label for="extra-curricular">Extra Curricular:</label>
        <textarea id="extra-curricular" placeholder="Your Extra Curricular Activities"></textarea>

        <label for="achievements">Achievements:</label>
        <textarea id="achievements" placeholder="Your Achievements"></textarea>

        <input type="submit" value="Submit">
    </form>
</div>



<%- include('partials/footer') %>